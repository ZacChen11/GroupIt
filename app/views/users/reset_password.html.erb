<h1>Reset Password</h1>
<%= form_for @user  do |f| %>
    <%= render partial: 'error_messages', object: @user, as: :item %>
    <%= f.label :password %>
    <%= f.password_field :password%>
    <br/><br/><%= f.label :password_confirmation%>
    <%= f.password_field :password_confirmation %>
    <br/><br/>
    <%= f.submit%>
<% end %>
